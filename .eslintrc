{
  "root": true,
  "env": {
    "es6": true,
    "jest": true
  },
  "extends": "airbnb",
  "plugins": ["prettier"],
  "globals": { "jest": true },
  "rules": {
    "func-names": 0,
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "prefer-const": 0,
    "arrow-parens": 0,
    "comma-dangle": 0
  }
}
